<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>AzureFunctions.Contrib.Redis</RootNamespace>
    <AssemblyName>AzureFunctions.Contrib.Redis</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageTags>Azure Functions Redis</PackageTags>
    <RepositoryUrl>https://github.com/AzureFunctionsContrib/AzureFunctions.Contrib.Redis</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/AzureFunctionsContrib/AzureFunctions.Contrib.Redis</PackageProjectUrl>
    <Description>Redis bindings for Azure Functions</Description>
    <Version>1.0.0-beta1</Version>
    <NeutralLanguage>en</NeutralLanguage>
    <Authors>fbeltrao</Authors>
    <Product>Azure Functions Contrib Extensions</Product>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.WebJobs.Extensions" Version="3.0.0-beta5" />
    <PackageReference Include="StackExchange.Redis.StrongName.Core" Version="1.1.605" />
  </ItemGroup>

</Project>
